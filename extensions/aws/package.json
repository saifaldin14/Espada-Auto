    },
    "./backup": {
      "import": "./dist/backup/index.js",
      "types": "./dist/backup/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "oxlint",
    "typecheck": "tsc --noEmit"
