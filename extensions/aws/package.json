  "version": "2026.1.27-beta.1",
  "description": "AWS Core Services plugin for Espada infrastructure management",
  "type": "module",
  "espada": {
    "extensions": [
      "./index.ts"
    ]
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
